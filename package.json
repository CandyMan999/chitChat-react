    "axios": "^0.16.2",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.17.2",
    "cloudinary": "^1.11.0",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.15.4",
    "express-formidable": "^1.2.0",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.0.17",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
